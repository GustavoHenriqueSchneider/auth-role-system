{
  "name": "auth-role-system",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "lint": "npx eslint \"**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "db:reset-migrations": "knex migrate:rollback --all && knex migrate:latest",
    "db:reset-data": "npx knex seed:run --specific=truncate_all.js",
    "db:initial-seed": "npx knex seed:run --specific=seed_initial.js",
    "db:reset": "npm run db:reset-migrations && npm run db:reset-data && npm run db:initial-seed",
    "db:generate-fake-user": "npx knex seed:run --specific=seed_create_fake_user.js",
    "db:terminal": "docker exec -it postgres-container psql -U auth_user -d auth_db",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "dev": "npm run docker:up && node --watch server.js",
    "start": "npm run docker:up && node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.18.2",
    "@faker-js/faker": "^9.7.0",
    "awilix": "^12.0.5",
    "awilix-express": "^9.0.2",
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "nodemailer": "^7.0.2",
    "pg": "^8.15.6",
    "redis": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
